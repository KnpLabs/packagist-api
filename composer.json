{
    "name": "knplabs/packagist-api",
    "type": "library",
    "description": "Packagist API client.",
    "keywords": ["packagist", "api", "composer"],
    "homepage": "http://knplabs.com",
    "license": "MIT",
    "authors": [
        {
            "name": "KnpLabs Team",
            "homepage": "http://knplabs.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "guzzle/http": "~3.0",
        "doctrine/inflector": "~1.0"
    },
    "require-dev": {
        "phpspec/phpspec2": "1.0.*@dev",
        "phpspec/php-diff": "*@dev"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "Packagist\\Api\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
